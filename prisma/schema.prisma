generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["relationJoins"]
}

datasource db {
    provider  = "postgresql"
    url       = env("DATABASE_URL")
    directUrl = env("DATABASE_DIRECT_URL")
}

model AiDetection {
    uid        String @id @unique @default(uuid())
    content    String
    documentId String
    userId     String

    createdAt DateTime @default(now())
}
